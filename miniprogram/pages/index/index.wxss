/* pages/login/index.wxss */
.clear{ clear: both; width: 100%; height: 0; content: '';overflow: hidden;}
.safety-main{  height: 100%; width: 100%; position:relative;}
.safety-main .top{ overflow: hidden; height: 100%; position: fixed; width: 100%; z-index: 4; }
.safety-main .top .img{ width: 100%; height: 100%;  margin: 0; padding: 0; float: left;}
.safety-main .cont{     position: absolute; width: 100%; left: 0;  z-index: 6;  top: 0; margin-top: 100%;}
.safety-main .cont .start,.safety-main .cont .end{ background:none; width: 88px;  height: 41px; padding:0; margin-top: 10px; }
.safety-main .cont .start image, .safety-main .cont .end image{ display: block; width: 100%; height: 100%;}